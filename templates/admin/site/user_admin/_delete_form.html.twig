<form class="d-grid" method="post" action="{{ path('user_admin_delete', {'id': user.id}) }}"
      onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer ce membre ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn btn-outline-danger my-3">Supprimer</button>
</form>
