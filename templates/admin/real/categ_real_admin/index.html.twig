{% extends 'base.html.twig' %}

{% block title %}CategReal index{% endblock %}

{% block body %}
    <h1>CategReal index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Slug</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categ_real in categ_reals %}
            <tr>
                <td>{{ categ_real.id }}</td>
                <td>{{ categ_real.name }}</td>
                <td>{{ categ_real.slug }}</td>
                <td>
                    <a href="{{ path('categ_real_admin_show', {'id': categ_real.id}) }}">show</a>
                    <a href="{{ path('categ_real_admin_edit', {'id': categ_real.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('categ_real_admin_new') }}">Create new</a>
{% endblock %}
